{
  "name": "numerix",
  "version": "0.2.0",
  "private": true,
  "description": "Une application d'apprentissage de l'arborescence des fichiers",
  "author": "Bxthesda",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
